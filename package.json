{
  "name": "lacona-command",
  "version": "0.2.2",
  "description": "Base Lacona Command which all Commands must extend",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p build; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonhorst/lacona-command.git"
  },
  "keywords": [
    "lacona",
    "app",
    "application",
    "launch"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-command/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-command",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "lacona": "^0.39.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc10",
    "elliptical-datetime": "^1.0.0-rc3",
    "moment": "^2.12.0"
  },
  "directories": {
    "test": "test"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
