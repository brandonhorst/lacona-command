{
  "name": "lacona-command",
  "version": "0.3.0",
  "description": "Alternative (incomplete) commands for Lacona",
  "lacona": {
    "extensions": "build/extensions.js"
  },
  "scripts": {
    "build:build": "mkdir -p build; browserify src/extensions.jsx -t babelify -o build/extensions.js -x lacona-phrases -x elliptical -x lacona-api --standalone extensions --extension=jsx",
    "build:lib": "babel src --out-dir lib",
    "clean": "rimraf build lib",
    "prepublish": "npm run clean && npm run build:build && npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonhorst/lacona-command.git"
  },
  "keywords": [
    "lacona",
    "app",
    "application",
    "launch"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-command/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-command",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-register": "^6.8.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "moment": "^2.13.0"
  },
  "directories": {
    "test": "test"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx"
    ],
    "presets": [
      "es2015"
    ]
  }
}
