{
  "name": "lacona-command",
  "version": "0.2.3",
  "description": "Base Lacona Command which all Commands must extend",
  "main": "lib-cjs/index.js",
  "jsnext:main": "lib-es/index.js",
  "scripts": {
    "build": "npm run build:es & npm run build:cjs & wait",
    "build:es": "mkdir -p build; cross-env BABEL_ENV=es babel src --out-dir lib-es",
    "build:cjs": "mkdir -p build; cross-env BABEL_ENV=cjs babel src --out-dir lib-cjs",
    "clean": "rimraf lib-es lib-cjs",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonhorst/lacona-command.git"
  },
  "keywords": [
    "lacona",
    "app",
    "application",
    "launch"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-command/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-command",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-register": "^6.8.0",
    "cross-env": "^1.0.7",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc13",
    "elliptical-datetime": "^1.0.0-rc3",
    "moment": "^2.13.0"
  },
  "directories": {
    "test": "test"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx"
    ],
    "env": {
      "cjs": {
        "presets": [
          "es2015"
        ]
      },
      "es": {
        "presets": [
          "es2015-native-modules"
        ]
      }
    }
  }
}
